{
  "compilerOptions": {
    "outDir": "./dist/",
    "pretty": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "target": "es2015", /* Node 6.11 (LTS) supports es2015*/
    "types": [
      "node"
    ]
  }
}
